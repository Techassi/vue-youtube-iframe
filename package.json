{
    "name": "@techassi/vue-youtube-iframe",
    "version": "1.0.6",
    "description": "This plugin makes it easy to integrate the YouTube Iframe API into your Vue 3 application",
    "author": "Techassi",
    "keywords": [
        "vue",
        "youtube",
        "iframe",
        "embed",
        "api",
        "youtube iframe api",
        "vue-youtube-iframe"
    ],
    "bugs": {
        "url": "https://github.com/Techassi/vue-youtube-iframe/issues"
    },
    "homepage": "https://github.com/Techassi/vue-youtube-iframe#readme",
    "files": [
        "dist"
    ],
    "main": "./dist/vue-youtube-iframe.umd.js",
    "module": "./dist/vue-youtube-iframe.es.js",
    "exports": {
        ".": {
            "import": "./dist/vue-youtube-iframe.es.js",
            "require": "./dist/vue-youtube-iframe.umd.js"
        }
    },
    "types": "./dist/types/index.d.ts",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "lint": "eslint --ext .js,.vue,.ts .",
        "lint:fix": "pnpm lint -- --fix",
        "build": "vue-tsc --noEmit && vite build",
        "dev": "vite"
    },
    "dependencies": {
        "vue": "^3.2.16"
    },
    "devDependencies": {
        "@techassi/eslint-config": "^0.0.3",
        "@types/node": "^16.11.7",
        "@types/youtube": "^0.0.46",
        "@vitejs/plugin-vue": "^1.9.3",
        "eslint": "^8.7.0",
        "typescript": "^4.4.3",
        "vite": "^2.6.4",
        "vite-plugin-dts": "^0.9.4",
        "vue-tsc": "^0.3.0"
    }
}