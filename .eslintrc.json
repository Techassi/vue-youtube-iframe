{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:vue/essential", "airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["vue", "prettier"],
    "rules": {
        "prettier/prettier": "error",
        "quotes": "error",
        "indent": ["error", 4]
    }
}
